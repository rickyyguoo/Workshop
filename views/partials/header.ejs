<body>
    
    <nav class="navbar navbar-expand-lg navbar-dark orange">
        <div class="container">
            <!-- Navbar brand -->
            <a class=" navbar-left" href="/home">
                   <img src="https://images.cooltext.com/5062430.png" width="219" height="52" alt="View Events" />
            </a>
            <!-- Collapse button -->
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
            <!-- Collapsible content -->
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <!-- Links -->
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item <%= typeof page !== 'undefined' && page === 'viewevents' ? 'active' : '' %>">
                        <a class="nav-link waves-effect waves-light" href="/viewevents">Events
                </a>
                    </li>
                    <li class="nav-item ">
                        <a class="nav-link waves-effect waves-light" href="/home">Home
                </a>
                    </li>
                 
                </ul>
    
                <div>
                    <ul class="nav navbar-nav navbar-right ">
                        <% if(!currentUser){ %>
                            <li class="<%= typeof page !== 'undefined' && page === 'login' ? 'active' : '' %>" nav-item>
                                <a href="/login" class="nav-link waves-effect waves-light">Login
                    </a>
                            </li>
                </div>
                <div>
                    <ul class="nav navbar-nav navbar-right ">
                        <li class="<%= typeof page !== 'undefined' && page === 'register' ? 'active' : '' %>" nav-item>
                            <a href="/register" class="nav-link waves-effect waves-light">Sign Up
                      </a>
                        </li>
                    </ul>
                </div>
                <% } else { %>
                    <li class="nav navbar-nav navbar-right"><a class="nav-link waves-effect waves-light" href="#">Hi ! <%= currentUser.username %></a></li>
                    <li class="nav navbar-nav navbar-right"><a class="nav-link waves-effect waves-light" href="/logout">Logout</a></li>
                    <% } %>
                        </ul>
            </div>
        </div>
    </nav>
