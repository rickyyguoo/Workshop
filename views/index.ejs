	<script>   
   $(document).ready(function()
   {
      $("#PanelMenu1").panel({animate: true, animationDuration: 200, animationEasing: 'linear', dismissible: true, display: 'overlay', position: 'right', toggle: true, overlay: true});
      $("a[href*='#intro']").click(function(event)
      {
         event.preventDefault();
         $('html, body').stop().animate({ scrollTop: $('#intro').offset().top }, 600, 'easeOutQuad');
      });
      function Heading2Scroll()
      {
         var $obj = $("#wb_Heading2");
         if (!$obj.hasClass("in-viewport") && $obj.inViewPort(false))
         {
            $obj.addClass("in-viewport");
            AnimateCss('wb_Heading1', 'vanish-in', 100, 1000);
         }
         else
         if ($obj.hasClass("in-viewport") && !$obj.inViewPort(true))
         {
            $obj.removeClass("in-viewport");
            AnimateCss('wb_Heading1', 'animate-fade-out', 0, 0);
         }
      }
      if (!$('#wb_Heading2').inViewPort(true))
      {
         $('#wb_Heading2').addClass("in-viewport");
      }
      Heading2Scroll();
      $(window).scroll(function(event)
      {
         Heading2Scroll();
      });
      var NewsFeedTicker1_Data = new Array();
      NewsFeedTicker1_Data[0] = ["http://", "Welcome to HMJSI Workshops &amp; Seminars !", "", "You can view lists of workshops &amp; seminars "];
      NewsFeedTicker1_Data[1] = ["http://", "Only at UPH Medan", "", "Proudly made by UPH Medan Student Intake 2016 , Information System."];
      NewsFeedTicker1_Data[2] = ["http://", "We need your feedback ! ", "", "You can also give a feedback about the Workshops &amp; Seminar "];
      $("#NewsFeedTicker1").newsviewer({ mode: 'rotate', pause: 3000, pause: 3000, animation: 1, animationDuration: 500, sortOrder: 0, dataSource: 'local', param: NewsFeedTicker1_Data, target: '_self', includeDate: false, maxItems: 10});
   });
</script>
